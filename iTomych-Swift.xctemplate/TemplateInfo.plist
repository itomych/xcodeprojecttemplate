<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Kind</key>
	<string>Xcode.Xcode3.ProjectTemplateUnitKind</string>
	<key>Identifier</key>
	<string>com.itomych.swift.template</string>
	<key>Ancestors</key>
	<array>
		<string>com.itomych.base.template</string>
	</array>
	<key>Concrete</key>
	<true/>
	<key>Description</key>
	<string>This template provides a starting point for an application with iOS department preferrable project structure</string>
	<key>SortOrder</key>
	<integer>1</integer>
	<key>Options</key>
	<array>
		<dict>
			<key>Identifier</key>
			<string>languageChoice</string>
			<key>Units</key>
			<dict>
				<key>Objective-C</key>
				<dict>
					<key>Nodes</key>
					<array>
					</array>
				</dict>
				<key>Swift</key>
				<dict>
					<key>Nodes</key>
					<array>
					</array>
				</dict>
			</dict>
		</dict>
	</array>
	<key>Targets</key>
	<array>
		<dict>
			<key>Name</key>
			<string>___PACKAGENAME___</string>
			<key>SharedSettings</key>
			<dict>
				<key>PRODUCT_NAME</key>
				<string>$(TARGET_NAME)</string>
			</dict>
			<key>BuildPhases</key>
			<array>
				<dict>
					<key>Class</key>
					<string>ShellScript</string>
					<key>Name</key>
					<string>SwiftGen</string>
					<key>ShellPath</key>
					<string>/bin/bash</string>
					<key>ShellScript</key>
					<string>&quot;${PODS_ROOT}/SwiftGen/bin/swiftgen&quot; config run --config &quot;${PROJECT_DIR}/AutomationTools/swiftgen.yml&quot;</string>
				</dict>
				<dict>
					<key>Class</key>
					<string>ShellScript</string>
					<key>Name</key>
					<string>SwiftLint</string>
					<key>ShellPath</key>
					<string>/bin/bash</string>
					<key>ShellScript</key>
					<string>&quot;${PODS_ROOT}/SwiftLint/swiftlint&quot; lint --config &quot;${PROJECT_DIR}/AutomationTools/swiftlint.yml&quot;</string>
				</dict>
				<dict>
					<key>Class</key>
					<string>ShellScript</string>
					<key>Name</key>
					<string>Copy-paste detector</string>
					<key>ShellPath</key>
					<string>/bin/bash</string>
					<key>ShellScript</key>
					<string>&quot;/usr/local/bin/pmd&quot; cpd --files ${EXECUTABLE_NAME} --minimum-tokens 50 --language swift --encoding UTF-8 --format net.sourceforge.pmd.cpd.XMLRenderer > cpd-output.xml --failOnViolation true
php &quot;./AutomationTools/cpd_script.php&quot; -cpd-xml cpd-output.xml</string>
				</dict>
			</array>
		</dict>
	</array>
	<key>Nodes</key>
	<array>
		<string>../AutomationTools/swiftlint.yml</string>
		<string>../AutomationTools/swiftgen.yml:basics</string>
		<string>../AutomationTools/cpd_script.php</string>
		<string>../Podfile:swiftbasics</string>
	</array>

	<key>Definitions</key>
	<dict>

		<key>../AutomationTools/swiftlint.yml</key>
		<dict>
			<key>Group</key>
			<array>
				<string>AutomationTools</string>
			</array>
			<key>Path</key>
			<string>swiftlint.yml</string>
			<key>TargetIndices</key>
			<array/>
		</dict>

		<key>../Podfile:swiftbasics</key>
		<string>pod &apos;SwiftLint&apos;
pod &apos;SwiftGen&apos;, &apos;~&gt; 5.3.0&apos;</string>

		<key>../AutomationTools/cpd_script.php</key>
		<dict>
			<key>Group</key>
			<array>
				<string>AutomationTools</string>
			</array>
			<key>Path</key>
			<string>cpd_script.php</string>
			<key>TargetIndices</key>
			<array/>
		</dict>

		<key>../AutomationTools/swiftgen.yml:basics</key>
		<string>xcassets:
   paths: ../___PACKAGENAME___/Assets.xcassets
   templateName: swift4
   output: ../___PACKAGENAME___/Resources/AssetsConstants.swift  
fonts:
   paths: ../___PACKAGENAME___/Resources/Fonts
   templateName: swift4
   output: ../___PACKAGENAME___/Resources/FontConstants.swift
storyboards:
   paths: ../___PACKAGENAME___/Flows
   templateName: swift4
   output: ../___PACKAGENAME___/Resources/StoryboardsConstants.swift</string>

	</dict>
</dict>
</plist>
