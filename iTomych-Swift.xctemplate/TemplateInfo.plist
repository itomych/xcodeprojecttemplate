<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Kind</key>
	<string>Xcode.Xcode3.ProjectTemplateUnitKind</string>
	<key>Identifier</key>
	<string>com.itomych.swift.template</string>
	<key>Ancestors</key>
	<array>
		<string>com.itomych.base.template</string>
	</array>
	<key>Concrete</key>
	<true/>
	<key>Description</key>
	<string>This template provides a starting point for an application with iOS department preferrable project structure</string>
	<key>SortOrder</key>
	<integer>1</integer>
	<key>Options</key>
	<array>
		<dict>
			<key>Identifier</key>
			<string>languageChoice</string>
			<key>Units</key>
			<dict>
				<key>Objective-C</key>
				<dict>
					<key>Nodes</key>
					<array>
					</array>
				</dict>
				<key>Swift</key>
				<dict>
					<key>Nodes</key>
					<array>
					</array>
				</dict>
			</dict>
		</dict>
	</array>
	<key>Targets</key>
	<array>
		<dict>
			<key>Name</key>
			<string>___PACKAGENAME___</string>
			<key>SharedSettings</key>
			<dict>
				<key>PRODUCT_NAME</key>
				<string>$(TARGET_NAME)</string>
			</dict>
			<key>BuildPhases</key>
			<array>
				<dict>
					<key>Class</key>
					<string>ShellScript</string>
					<key>Name</key>
					<string>SwiftGen</string>
					<key>ShellPath</key>
					<string>/bin/bash</string>
					<key>ShellScript</key>
					<string>&quot;${PODS_ROOT}/SwiftGen/bin/swiftgen&quot; config run --config &quot;${PROJECT_DIR}/AutomationTools/swiftgen.yml&quot;</string>
				</dict>
				<dict>
					<key>Class</key>
					<string>ShellScript</string>
					<key>Name</key>
					<string>SwiftLint</string>
					<key>ShellPath</key>
					<string>/bin/bash</string>
					<key>ShellScript</key>
					<string>&quot;${PODS_ROOT}/SwiftLint/swiftlint&quot; lint --config &quot;${PROJECT_DIR}/AutomationTools/swiftlint.yml&quot;</string>
				</dict>
			</array>
		</dict>
	</array>
	<key>Nodes</key>
	<array>
		<string>../Podfile</string>
		<string>../AutomationTools/swiftlint.yml</string>
		<string>../AutomationTools/swiftgen.yml:basics</string>
	</array>

	<key>Definitions</key>
	<dict>
		<key>../Podfile</key>
		<dict>
		  <key>TargetIndices</key>
		  <array/>
		  <key>Beginning</key>
		  <string>source &apos;https://github.com/CocoaPods/Specs.git&apos;
platform :ios, &apos;10.0&apos;
use_frameworks!
target &apos;___PACKAGENAME___&apos; do</string>
		  <key>End</key>
		  <string>pod &apos;SwiftLint&apos;
pod &apos;SwiftGen&apos;, &apos;~&gt; 5.3.0&apos; 
end
		  </string>
		</dict>

		<key>../AutomationTools/swiftlint.yml</key>
		<dict>
			<key>Group</key>
			<array>
				<string>AutomationTools</string>
			</array>
			<key>Path</key>
			<string>swiftlint.yml</string>
			<key>TargetIndices</key>
			<array/>
		</dict>

		<key>../AutomationTools/swiftgen.yml:basics</key>
		<string>xcassets:
   paths: ../___PACKAGENAME___/Assets.xcassets
   templateName: swift4
   output: ../___PACKAGENAME___/Resources/AssetsConstants.swift  
fonts:
   paths: ../___PACKAGENAME___/Resources/Fonts
   templateName: swift4
   output: ../___PACKAGENAME___/Resources/FontConstants.swift
storyboards:
   paths: ../___PACKAGENAME___/Flows
   templateName: swift4
   output: ../___PACKAGENAME___/Resources/StoryboardsConstants.swift</string>

	</dict>
</dict>
</plist>
